<div class="webmaker-create-user">
  <form id="sso_create" action="<%= process.env.AUDIENCE %>/user" method="POST">
    <legend>Claim your webmaker space</legend>
    <div class="display-subdomain-container row">
      <label for="subdomain" class="visually-hidden">Display subdomain</label>
      <input class="display-subdomain-input" type="text" name="subdomain" id="subdomain" required> .webmaker.org
    </div>
    <div class="row">
      <input type="checkbox" name="terms" id="terms" class="inline" required>
      <label for="terms" class="inline">I agree to your terms and conditions</label>
    </div>
    <input class="create-user-submit" type="submit" value="Create my account">
  </form>
</div>
